<Window x:Class="TESTETOPBAR.pages.Copia"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TESTETOPBAR.pages"
        mc:Ignorable="d"
        Title="Copia" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtsource" HorizontalAlignment="Left" Margin="494,11,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="168" Height="31"/>
        <Button Content="Select file&#xA;" HorizontalAlignment="Left" Margin="681,11,0,0" VerticalAlignment="Top" RenderTransformOrigin="4.099,-4.657" Height="28" Width="82" Click="Button_Click"/>
        <Label Content="FIle name&#xD;&#xA;&#xA;" HorizontalAlignment="Left" Margin="412,11,0,0" VerticalAlignment="Top"/>
        <Button Content="Copy&#xA;" HorizontalAlignment="Left" Margin="668,73,0,0" VerticalAlignment="Top" RenderTransformOrigin="4.099,-4.657" Height="43" Width="108" Click="Button_Click_1"/>
        <ListView d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,24,542,32" SelectionChanged="ListView_SelectionChanged">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
